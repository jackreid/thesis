@article{tomlinsonPRESIDENTIALADDRESSGEOGRAPHIC1989,
  title = {{{PRESIDENTIAL ADDRESS}}: {{GEOGRAPHIC INFORMATION SYSTEMS}} and {{GEOGRAPHERS IN THE}} 1990s},
  shorttitle = {{{PRESIDENTIAL ADDRESS}}},
  author = {Tomlinson, Roger F.},
  year = {1989},
