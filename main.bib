@incollection{cramptonRethinkingMapsIdentity2011,
  title = {Rethinking Maps and Identity},
  booktitle = {Rethinking {{Maps}}: {{New Frontiers}} in {{Cartographic Theory}}},
  author = {Crampton, Jeremy},
