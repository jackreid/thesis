@article{mousivandGlobalSensitivityAnalysis2014,
  title = {Global Sensitivity Analysis of the Spectral Radiance of a Soil\textendash Vegetation System},
  author = {Mousivand, Alijafar and Menenti, Massimo and Gorte, Ben and Verhoef, Wout},
  year = {2014},
