@article{tannerMangrovesGalapagosEcosystem2019,
  title = {Mangroves in the {{Galapagos}}: {{Ecosystem}} Services and Their Valuation},
  shorttitle = {Mangroves in the {{Galapagos}}},
  author = {Tanner, Michael K. and Moity, Nicolas and Costa, Matthew T. and Marin Jarrin, Jose R. and {Aburto-Oropeza}, Octavio and {Salinas-de-Le{\'o}n}, Pelayo},
