  language = {en},
  keywords = {Biography \& Autobiography / Personal Memoirs,Biography \& Autobiography / Women,Social Science / Media Studies}
}

@article{ulibarriCollaborativeModelDevelopment2018,
  title = {Collaborative Model Development Increases Trust in and Use of Scientific Information in Environmental Decision-Making},
  author = {Ulibarri, Nicola},
  year = {2018},
  month = apr,
  journal = {Environmental Science \& Policy},
  volume = {82},
  pages = {136--142},
  issn = {1462-9011},
  doi = {10.1016/j.envsci.2018.01.022},
  abstract = {While science matters for environmental management, creating science that is credible, salient to decision-makers, and deemed legitimate by stakeholders is challenging. Collaborative modeling is an increasingly-used approach to enable effective science-based decision-making. This work evaluates the modeling process conducted for two hydropower dam licensing negotiations, to explore how differences in the collaborative development of hydrological models affected differences in their use in subsequent decision-making. In one case, the model was developed iteratively through deliberation with stakeholders. Consequently, stakeholders understood the model and its limitations and trusted the model and modelers; the model itself was also better designed to evaluate resource managers' questions. The collaboratively-developed model became the focal point for subsequent negotiations and enabled creative group problem-solving. Conversely, in the case with less engagement during model development, the model was not used subsequently by decision-makers. These differences are argued to result from trust built during the modeling process, applicability of the model to test real management scenarios, and the broader social context in which the models were used.},
  language = {en},
  keywords = {Collaborative modeling,FERC hydropower relicensing,Hydropower,Participatory modeling,Science-policy interface,Water management},
  file = {/home/jackreid/Zotero/storage/YMFJZCLK/Ulibarri - 2018 - Collaborative model development increases trust in.pdf;/home/jackreid/Zotero/storage/NHIB6CTN/S1462901117308262.html}
