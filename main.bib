@article{purvisThermodynamicEntropyIndicator2017,
  title = {Thermodynamic {{Entropy}} as an {{Indicator}} for {{Urban Sustainability}}?},
  author = {Purvis, Ben and Mao, Yong and Robinson, Darren},
  year = {2017},
  month = jan,
  journal = {Procedia Engineering},
  series = {Urban {{Transitions Conference}}, {{Shanghai}}, {{September}} 2016},
  volume = {198},
  pages = {802--812},
  issn = {1877-7058},
  doi = {10.1016/j.proeng.2017.07.131},
  abstract = {As foci of economic activity, resource consumption, and the production of material waste and pollution, cities represent both a major hurdle and yet also a source of great potential for achieving the goal of sustainability. Motivated by the desire to better understand and measure sustainability in quantitative terms we explore the applicability of thermodynamic entropy to urban systems as a tool for evaluating sustainability. Having comprehensively reviewed the application of thermodynamic entropy to urban systems we argue that the role it can hope to play in characterising sustainability is less general than has been suggested in the past. We show that thermodynamic entropy may be considered as a measure of energy efficiency, but must be complimented by other indices to form part of a broader measure of urban sustainability.},
  langid = {english},
  keywords = {city,entropy,exergy,indicators,second law,sustainability,thermodynamics},
  file = {/home/jackreid/Zotero/storage/532BBQD7/Purvis et al_2017_Thermodynamic Entropy as an Indicator for Urban Sustainability.pdf;/home/jackreid/Zotero/storage/M32A3PP3/S1877705817329788.html}
}

@article{qiuSupplyHubIndustrial2013,
  title = {Supply {{Hub}} in {{Industrial Park}} ({{SHIP}}): {{The}} Value of Freight Consolidation},
  shorttitle = {Supply {{Hub}} in {{Industrial Park}} ({{SHIP}})},
  author = {Qiu, Xuan and Huang, George Q.},
  year = {2013},
  month = may,
  journal = {Computers \& Industrial Engineering},
  series = {Intelligent {{Manufacturing Systems}}},
  volume = {65},
  number = {1},
  pages = {16--27},
  issn = {0360-8352},
  doi = {10.1016/j.cie.2012.06.016},
  abstract = {Industrial parks, characterized as a cluster of enterprises situated in one location to share common resources, have played an indispensible role in boosting regional economic and industrial development. However, further development has been impeded by the shortage of land space, especially for the construction of warehouses. This paper proposes the concept of Supply Hub in Industrial Park (SHIP) as a promising approach addressing this challenge. SHIP is defined as a public provider of warehousing and logistics services for manufacturing enterprises located within an industrial park. The research reported in this paper focuses on evaluating the value of freight consolidation, one of the typical benefits of applying the SHIP approach. Two mathematical models of the supply chain in a typical industrial park are formulated: with and without SHIP. Genetic Algorithm (GA) is applied for solving the two models, and sensitivity experiments are conducted for comparative analysis between the two scenarios. The computational results show that through consolidating shipments, SHIP brings benefits to the whole industrial park. Total cost savings resulted from the application of SHIP would increase with the size of the supply chain, the vehicle capacity, and the rates of fixed transportation costs and holding costs of finished products at manufacturers.},
  langid = {english},
  keywords = {Freight consolidation,Genetic algorithm,Industrial park,Public warehouse,Supply hub},
  file = {/home/jackreid/Zotero/storage/UX6NRU2N/S0360835212001751.html}
}

