  title = {{{THE POTENTIAL OF BUILDING DETECTION FROM SAR AND LIDAR USING DEEP LEARNING}}},
  booktitle = {The {{International Archives}} of the {{Photogrammetry}}, {{Remote Sensing}} and {{Spatial Information Sciences}}},
  author = {Nordin, Z. and Shafri, H. Z. M. and Abdullah, A. F. and Hashim, S. J.},
