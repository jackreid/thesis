@article{woodChartingEvolutionSatellite2012,
  title = {Charting the Evolution of Satellite Programs in Developing Countries - {{The Space Technology Ladder}}},
  author = {Wood, Danielle and Weigel, Annalisa},
  year = {2012},
  journal = {Space Policy},
  volume = {28},
