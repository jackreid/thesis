@inproceedings{kaulNextGenerationUrban2011,
  title = {Next Generation Urban - {{Unmanned}} Air System},
  booktitle = {{{AUVSI Unmanned Systems North America Conference}} 2011, {{August}} 16, 2011  -  {{August}} 19, 2011},
