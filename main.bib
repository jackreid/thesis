@article{longoValueOrientedEthicalTechnology2020,
  title = {Value-{{Oriented}} and {{Ethical Technology Engineering}} in {{Industry}} 5.0: {{A Human-Centric Perspective}} for the {{Design}} of the {{Factory}} of the {{Future}}},
  shorttitle = {Value-{{Oriented}} and {{Ethical Technology Engineering}} in {{Industry}} 5.0},
  author = {Longo, Francesco and Padovano, Antonio and Umbrello, Steven},
  year = {2020},
  month = jun,
  journal = {Applied Sciences},
  volume = {10},
  number = {12},
  pages = {4182},
  issn = {2076-3417},
  doi = {10.3390/app10124182},
  abstract = {Although manufacturing companies are currently situated at a transition point in what has been called Industry 4.0, a new revolutionary wave\textemdash Industry 5.0\textemdash is emerging as an `Age of Augmentation' when the human and machine reconcile and work in perfect symbiosis with one another. Recent years have indeed assisted in drawing attention to the human-centric design of Cyber-Physical Production Systems (CPPS) and to the genesis of the `Operator 4.0', two novel concepts that raise significant ethical questions regarding the impact of technology on workers and society at large. This paper argues that a value-oriented and ethical technology engineering in Industry 5.0 is an urgent and sensitive topic as demonstrated by a survey administered to industry leaders from different companies. The Value Sensitive Design (VSD) approach is proposed as a principled framework to illustrate how technologies enabling human\textendash machine symbiosis in the Factory of the Future can be designed to embody elicited human values and to illustrate actionable steps that engineers and designers can take in their design projects. Use cases based on real solutions and prototypes discuss how a design-for-values approach aids in the investigation and mitigation of ethical issues emerging from the implementation of technological solutions and, hence, support the migration to a symbiotic Factory of the Future.},
  langid = {english},
  file = {/home/jackreid/Zotero/storage/INDU5R2T/Longo et al. - 2020 - Value-Oriented and Ethical Technology Engineering .pdf}
}

