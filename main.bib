  langid = {english},
  keywords = {mangroves,meta-regression analysis,nonmarket valuation},
  file = {/home/jackreid/Zotero/storage/N9IQUX3L/Salem_Mercer_2012_The Economic Value of Mangroves.pdf;/home/jackreid/Zotero/storage/S5ZZK7BP/359.html}
