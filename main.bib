  keywords = {Coupled radiative transfer model,Global sensitivity analysis,Imagine spectroscopy,MODTRAN,SLC,Soil–vegetation system,TOA radiance,Winding stairs sampling},
  file = {/home/jackreid/Zotero/storage/NWSSIMT7/S0034425714000455.html}
