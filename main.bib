@article{gorodSystemofSystemsEngineeringManagement2008,
  title = {System-of-{{Systems Engineering Management}}: {{A Review}} of {{Modern History}} and a {{Path Forward}}},
  shorttitle = {System-of-{{Systems Engineering Management}}},
